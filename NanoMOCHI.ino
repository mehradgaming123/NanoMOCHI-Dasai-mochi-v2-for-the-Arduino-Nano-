// üîß Setup
void setup() {
  u8g2.begin();
  randomSeed(analogRead(0));
  showIntro();
  showIdle();
}

// üîÅ Loop
void loop() {
  delay(random(2000, 6000)); // Wait 2‚Äì6 seconds
  int expr = random(0, NUM_EXPRESSIONS);
  playExpression(expr);
  showIdle();
}

// üì∫ Intro screen
void showIntro() {
  u8g2.clearBuffer();
  u8g2.setFont(u8g2_font_ncenB08_tr);
  u8g2.drawStr(30, 30, "NanoMochi");
  u8g2.sendBuffer();
  delay(2500);
}

// üòê Idle frame
void showIdle() {
  drawFrame(mochiFrames[14]);
}

// üé≠ Play expression
void playExpression(int expr) {
  int start = EXPRESSION_START_INDEX[expr];
  int count = NUM_FRAMES[expr];
  for (int i = 0; i < count; i++) {
    drawFrame(mochiFrames[start + i]);
    delay(frameDelay);
  }
}

// üñºÔ∏è Draw frame
void drawFrame(const uint8_t* bitmap) {
  u8g2.clearBuffer();
  u8g2.drawXBMP(0, 0, 128, 64, bitmap);
  u8g2.sendBuffer();
}